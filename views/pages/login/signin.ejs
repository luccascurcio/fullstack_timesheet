<h1>Sign In</h1>

<form method="post" action="/session/create" class="needs-validation">
  <div class="form-group">
    <label for="email">Email</label>
    <input required type="email" class="form-control" id="email" name="email" aria-describedby="emailHelp" placeholder="Enter email">
  </div>
  <div class="form-group">
    <label for="password">Password</label>
    <input required type="password" class="form-control" id="password" name='password' placeholder="Password">
  </div>
  <br />
  <button type="submit" class="btn btn-primary">Login</button>
  <a href="/signup" class="btn btn-danger">Sign Up</a>
</form>

