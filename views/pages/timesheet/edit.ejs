<h2>Edit My Timesheet</h2>
<form method="post" action="/timesheetUpdate/<%= id %>">
    
  <div class="form-group row">
    <div class="col-sm-10">
        <label>Type</label>
        <input type="text" name="type" class="form-control" disabled value="<%= type %>">
    </div>
  </div> 
  
  <div class="form-group row">
    <div class="col-sm-10">
        <label>Date</label>
        <input type="date" name="date" class="form-control" readonly value="<%= date %>">
    </div>
  </div>   
  
  <div class="form-group row">
      <div class="col-sm-10">
          <label>Time</label>
          <input format type="time" name="time" class="form-control" value="<%= time %>">
      </div>
    </div>  

    <br />
    <div class="form-group row">
        <div class="col-sm-10">
          <button type="submit" class="btn btn-primary">Submit</button>
          <a href="/timesheetIndex/" class="btn btn-danger">Cancel</a>
        </div>
      </div>
</form>